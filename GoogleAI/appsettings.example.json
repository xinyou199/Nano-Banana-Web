{
  "TaskProcessorSettings": {
    "MaxConcurrentTasks": 10,
    "QueueCapacity": 2000,
    "TaskTimeoutMinutes": 15,
    "ProgressUpdateIntervalSeconds": 10,
    "ProgressUpdateMinDelta": 15,
    "StuckTaskDetectionMinutes": 10,
    "MaxImageSizeMB": 8,
    "JpegQuality": 90,
    "ThumbnailWidth": 1080,
    "ThumbnailHeight": 1080,
    "ThumbnailQuality": 80
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;port=3306;database=googleai;user=root;password=your_password;CharSet=utf8mb4;"
  },
  "CloudflareR2": {
    "ApiBaseUri": "https://your-r2-endpoint.r2.cloudflarestorage.com",
    "AccessKeyId": "your_access_key",
    "Secret": "your_secret_key",
    "ApiToken": "your_api_token",
    "BucketName": "your_bucket",
    "Region": "auto",
    "PresignedUrlExpiryInMinutes": 15,
    "HostUrl": "https://your-host-url.com"
  },
  "JwtSettings": {
    "SecretKey": "YourSecretKeyHere_ChangeThisToASecureRandomString",
    "Issuer": "GoogleAI",
    "Audience": "GoogleAIUsers",
    "ExpirationMinutes": 1440
  },
  "SmtpSettings": {
    "Host": "smtp.example.com",
    "Port": 465,
    "EnableSsl": true,
    "UserName": "your_email@example.com",
    "Password": "your_email_password",
    "FromEmail": "your_email@example.com"
  },
  "WeChatPay": {
    "AppId": "wx1234567890abcdef",
    "MerchantId": "1234567890",
    "ApiV3Key": "your_api_v3_key_32_characters",
    "SerialNo": "your_certificate_serial_number",
    "PrivateKeyPath": "certificates/apiclient_key.pem",
    "NotifyUrl": "https://yourdomain.com/api/wechatpaynotify/payment"
  }
}
